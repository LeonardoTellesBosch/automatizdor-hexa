import pyautogui
import time
dia = input('escreva o momento desejado: ')           pt-br
fim = input('escreva o momento final: ')

pyautogui.PAUSE = 2


 



pyautogui.hotkey("win")
pyautogui.write("google chrome")
pyautogui.press("enter")
pyautogui.click(x=619, y=59)
pyautogui.write("10.0.0.20") // essa linha destina a se a colocar o link ou ip do hexa//
pyautogui.hotkey("enter")
time.sleep (1)
pyautogui.click(x=663, y=477)
import pyautogui
import time
dia = input('escreva o momento desejado: ') // escreva a data que voce quer que pegue no inicio do periodo e na subsequente o fim do periodo e pressione enter
fim = input('escreva o momento final: ')

pyautogui.PAUSE = 2


 



pyautogui.hotkey("win")
pyautogui.write("google chrome")
pyautogui.press("enter")
pyautogui.click(x=619, y=59)
pyautogui.write("10.0.0.20")
pyautogui.hotkey("enter")
time.sleep (1)
pyautogui.click(x=663, y=477)
pyautogui.write("xxxxx")  //coloque seu email ou nome no email que usa no hexa
pyautogui.click(x=664, y=516)
pyautogui.write("xxxxx") //  coloque a senha
time.sleep (0.5)
pyautogui.press("enter")
pyautogui.click(x=633, y=137)
time.sleep (2)
#pyautogui.click(button = 'left')
pyautogui.click(x=430, y=456)
pyautogui.click(button = 'left')
pyautogui.click(x=697, y=408)
pyautogui.hotkey("ctrl", "a")
pyautogui.write(dia)
pyautogui.click(x=700, y=441)
pyautogui.hotkey("ctrl", "a")
pyautogui.write(fim)
pyautogui.click(x=742, y=476)

